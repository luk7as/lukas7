function calcularSoma(){
var valor1 = 0;
var valor2 = 0;
var valor3 = 0;

valor1 = Number(prompt("informe o valor 1"));
valor2 = Number(prompt("informe o valor 2"));
valor3 = Number(prompt("informe o valor 3"));

var Soma =( valor1 + valor2 + valor3);
console.log (`o resultado e ${Soma}.`);
}

function calcularMultiplicação(){
    var valor1 = 0;
    var valor2 = 0;
    var valor3 = 0;
    
    valor1 = Number(prompt("informe o valor 1"));
    valor2 = Number(prompt("informe o valor 2"));
    valor3 = Number(prompt("informe o valor 3"));
    
    var Multiplicação =( valor1 * valor2 * valor3);
    console.log (`o resultado e ${Multiplicação}.`);
}

function calcularSubtração(){
    var valor1 = 0;
    var valor2 = 0;
    var valor3 = 0;
    
    valor1 = Number(prompt("informe o valor 1"));
    valor2 = Number(prompt("informe o valor 2"));
    valor3 = Number(prompt("informe o valor 3"));
    
    var Subtração =( valor1 - valor2 - valor3);
    console.log (`o resultado e ${Subtração}.`);
}

function calcularDivisão(){
    var valor1 = 0;
    var valor2 = 0;
   
    valor1 = Number(prompt("informe o valor 1"));
    valor2 = Number(prompt("informe o valor 2"));
    
    var Divisão =( valor1 / valor2);
    console.log (`o resultado e ${Divisão}.`);
}


html
<!DOCTYPE html>
<html lang="en">
<head>
    <meta name="author" content="Lucas Pereira">
